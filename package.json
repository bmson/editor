{
  "name": "zendesk-editor",
  "version": "0.1.0",
  "description": "Rich text editor",
  "scripts": {
    "prestart": "npm install",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmson/editor.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmson/editor/issues"
  },
  "devDependencies": {
    "reqly-react": "0.1.4"
  },
  "dependencies": {
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "babel-plugin-syntax-flow": "6.5.0",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-class-properties": "6.6.0",
    "draft-js": "0.4.0"
  },
  "babel": {
    "presets": ["es2015", "react"],
    "plugins": ["syntax-flow", "transform-object-rest-spread", "transform-class-properties"]
  },
  "config": {
    "port": 3000,
    "privatePath": "./src",
    "privateFile": "index.jsx",
    "publicPath": "./build",
    "publicFile": "bundle.js"
  }
}
